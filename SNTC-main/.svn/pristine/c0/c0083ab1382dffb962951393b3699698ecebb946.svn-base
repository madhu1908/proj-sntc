.loginpage .loginpage-formwrapper
{
    background: #fff;
    border-radius: 15px;
    border: 1px solid #9c9a9a;
    width: 450px !important;
    overflow: hidden;
    box-shadow: 0 0 8px 0 rgb(0 0 0 / 20%), 0 0 20px 0 rgb(0 0 0 / 19%);
    padding: 40px;
    // position: absolute;
    right: 168px;
    margin: -250px auto;
    @media (min-width: 300px)
    {
    width: 300px !important;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  right: 0;
  margin: 10px auto;
  padding: 10px;
    }
    @media (min-width: 350px)
    {
        width: 350px !important;
        top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  right: 0;
  margin: 10px auto;
  padding: 10px;
    }
    @media (min-width: 400px)
    {
        width: 400px !important;
        top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  right: 0;
  margin: 10px auto;
  padding: 10px;
    }
    @media (min-width: 900px)
    {
        width: 300px !important;
    }
    @media (min-width: 1024px)
    {
        width: 350px !important;
        padding: 20px;
        right: -41%;
        transform: none;
        top: 10%;
        left: 0%;
    }
    @media (min-width: 1200px)
    {
        width: 400px !important;
        padding: 40px;
        right: -41%;
        transform: none;
        top: 10%;
        left: 0%;
    }
    @media (min-width: 1280px)
    {
        width: 450px !important;
        padding: 40px;
        right: -41%;
        transform: none;
        top: 10%;
        left: 0%;
    }
}
.login-logo
{
    text-align: center;
}
.login-logo .logo-left img
{
    width: 50%;
}
.login-logo .logo-right img
{
    width: 80%;
    float: right;
}
.loginpage-right h2
 {
        margin: 20px 0 8px 0;
        color: #0a1325;
        font-weight: 600;
        line-height: 1.3;
        text-align: center;
        font-size: 22px;
        font-family: 'Dubai';
        @media (min-width: 900px)
        {
            font-size: 18px;
        }
        @media (min-width: 1024px)
        {
            font-size: 19px;
        }
        @media (min-width: 1200px)
        {
            font-size: 21px;
        }
        @media (min-width: 1280px)
        {
            font-size: 22px;
        }
    }
.loginpage-form
{
    padding-left: 25px;
    padding-right: 25px;
    @media (min-width: 300px)
    {
    padding-left: 10px;
    padding-right: 10px; 
    }
}
.loginpage-form .form-control {
    display: flex;
    flex: 1;
    min-width: 50px;
    height: auto;
    padding: 8px 8px;
    transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
    color: #b3b3b3;
    border-top: none;
    border-left: none;
    border-right: none;
    border-bottom: 2px solid #919191;
    border-radius: 0px;
    background-color: transparent !important;
    background-image: none;
    box-shadow: none;
    font-size: 16px;
    line-height: 1.428571429;
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    font-family: 'Dubai';
    font-weight: 500;
    @media (min-width: 300px)
    {
        font-size: 12px;
    }
    @media (min-width: 900px)
        {
            font-size: 12px;
        }
        @media (min-width: 1024px)
        {
            font-size: 14px;
        }
        @media (min-width: 1200px)
        {
            font-size: 15px;
        }
        @media (min-width: 1280px)
        {
            font-size: 16px;
        }
}
.loginpage-form .form-control::placeholder
{
    color: #b3b3b3;
    font-family: 'Dubai';
}
.loginpage-form .form-group .inputwrapper .glyphicon {
    position: absolute;
    top: 50%;
    left: 8px;
    transform: translateY(-50%);
    color: #b3b3b3;
}
.loginpage-form input[type=checkbox]:before {
    width: 100%;
    height: 100%;
    content: "";
    border: 2px solid #979797;
    border-radius: 0px;
    background-color: transparent;
}
.loginpage-form .checkbox label {
    min-height: 20px;
    padding-left: 0px;
    margin-bottom: 0;
    font-weight: normal;
    cursor: pointer;
    color: #b3b3b3;
    font-family: 'Dubai';
    font-weight: 500;
    font-size: 16px;
    vertical-align: middle;
    @media (min-width: 300px)
    {
        font-size: 12px;
    }
    @media (min-width: 900px)
        {
            font-size: 12px;
        }
        @media (min-width: 1024px)
        {
            font-size: 14px;
        }
        @media (min-width: 1200px)
        {
            font-size: 15px;
        }
        @media (min-width: 1280px)
        {
            font-size: 16px;
        }
}
.loginpage-form  input[type=checkbox] {
    position: relative !important;
    width: 20px;
    height: 20px;
    margin: 0 !important;
    cursor: pointer;
    -webkit-user-select: none;
    user-select: none;
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    margin-right: 10px !important;
    vertical-align: middle;
}
.loginpage-form .btn {
    display: inline-block;
    margin-bottom: 0;
    padding: 0.6em 1em;
    cursor: pointer;
    -webkit-user-select: none;
    user-select: none;
    transition: all 0.2s ease-in-out;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;
    color: white;
    border: none;
    border-radius: 30px;
    /*background-color: #faa316;*/
    background-image: linear-gradient(to right, #00617D, #039dc8);
    box-shadow: none;
    text-shadow: none;
    font-size: 16px;
    line-height: 1.428571429;
    font-family: 'Dubai';
    font-weight: 600;
    width: 100%;
    @media (min-width: 300px)
    {
        font-size: 12px;
    }
    @media (min-width: 900px)
        {
            font-size: 12px;
        }
        @media (min-width: 1024px)
        {
            font-size: 14px;
        }
        @media (min-width: 1200px)
        {
            font-size: 15px;
        }
        @media (min-width: 1280px)
        {
            font-size: 16px;
        }
}
.loginpage-form .btn .btn-success {
    color: #000;
    background-color: #faa316;
    border-color: #faa316;
}
.loginpage-form .btn .glyphicon
{
    padding-right: 10px;
}
.loginpage-form .or
{
    padding: 10px 0px;
    font-family: 'Dubai';
    font-weight: 500;
    font-size: 12px;
}
.loginpage-form .btna {
    display: inline-block;
    margin-bottom: 0;
    padding: 0.6em .5em;
    cursor: pointer;
    -webkit-user-select: none;
    user-select: none;
    transition: all 0.2s ease-in-out;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;
    color: #fff;
    border: none;
    border-radius: 30px;
    background-color: none;
    background-image: linear-gradient(to right, #02606a , #2aa6ae);
    box-shadow: none;
    text-shadow: none;
    font-size: 16px;
    line-height: 1.428571429;
    font-family: 'Dubai';
    font-weight: 100;
    @media (min-width: 300px)
    {
        font-size: 12px;
    }
    @media (min-width: 900px)
        {
            font-size: 12px;
        }
        @media (min-width: 1024px)
        {
            font-size: 14px;
        }
        @media (min-width: 1200px)
        {
            font-size: 15px;
        }
        @media (min-width: 1280px)
        {
            font-size: 16px;
        }
}
.loginpage-form .btna .btn-successa {
    color: #fff !important;
    background-color: #02606a !important;
    border-color: #02606a !important;
}

.loginpage-form .btna img {
    width: 10%;
}
.loginpage-form input[type=checkbox]:after {
    width: 8px;
    height: 4px;
    margin: 6px 5px;
    transform: rotate(-45deg);
    pointer-events: none;
    border: 2px solid blue;
    border-top: 0;
    border-right: 0;
}
.form-control:not([readonly]):focus {
   border-color: none;
}
.loginpage-form .form-group .inputwrapper .form-control:focus ~ .glyphicon:before {
    color: #b3b3b3;
}
.glyphicon:hover {
    background-color: none !important;
}

.logg{
     .control-label {
        flex: 4;
        margin-bottom: 0;
        font-size: 14px;
        font-weight: 500;
        display:none;
     }
}