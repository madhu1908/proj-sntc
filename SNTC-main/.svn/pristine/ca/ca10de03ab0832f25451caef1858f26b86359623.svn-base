.c-newlayout {
    .region-topbar {
        position: relative;
        z-index: 2;
        min-height: 48px;
        background: white;
        box-shadow: 0px 0px 6px #dedede;
    }

    .region-topbar .topbar-content {
        align-items: center;
        min-height: 48px;
        justify-content: center;
        display: block;
    }

    .toggle-btn img {
        width: 35px;
        height: auto;
        margin-top: 25px;
    }

    .region-sidebar {
        background-color: #ffffff;
        height: 88.5vh;
    }

    .mx-navigationtree .navbar-inner>ul>li>a {
        background-color: #ffffff;
    }

    .region-sidebar .mx-navigationtree .navbar-inner>ul>li a {
        font-weight: 600;
    }

    .region-sidebar .mx-navigationtree .navbar-inner>ul>li>a:hover {
        background-color: #dcd7d6;
        color: #000000 !important;
        font-weight: 600;
    }

    .region-sidebar .mx-navigationtree .navbar-inner>ul>li a.active {
        background-color: #dcd7d6;
        color: #646464 !important;
        font-weight: 600;

        .glyphicon {
            color: #646464 !important;
        }

    }

    a:focus {
        outline: none;
    }

    .mx-scrollcontainer-wrapper .navbar-inner .glyphicon:hover,
    .mx-scrollcontainer-wrapper .navbar-inner .glyphicon:active {
        background-color: #dcd7d6!important;
        margin: 5px;
        width: 42px !important;
        height: 42px !important;
        color: #606060 !important;
        /* box-shadow: 0px 0px 0px #e3e3e3; */
    }

    .mx-scrollcontainer-wrapper .navbar-inner .glyphicon {
        color: #666666;
        margin: 5px;
        width: 42px !important;
        height: 42px !important;
    }

    .region-sidebar {

        z-index: 101;
        box-shadow: 0 0 0px rgb(0 0 0 / 14%), 2px 3px 6px rgb(192 192 192 / 46%);
    }
}

