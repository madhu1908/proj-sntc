//table search
.mx-grid-searchbar {
    padding: 20px 5px;
    background: #ffffff;
    border-radius: 15px;
}

.s-btn,
.mx-grid-search-button,
.mx-grid-reset-button {
    background: linear-gradient(360deg, #0054a8, #6eaaf5);
    border-radius: 10px;
    border: none;
    color: white;
    min-width: 110px;
    font-size: 13px;
    // box-shadow: 0px 0px 14px #84848447;
    margin: 0px 20px;
}

.s-btn:hover, .s-btn:focus, .s-btn:active{
    color: #ffffff;
    border-color: #e7e7e9;
    background: linear-gradient(360deg, #004488, #5689c8);
    // box-shadow: 0px 0px 14px #84848447;
}




.mx-grid-search-item {
    display: flex;
    flex-direction: column;

    .mx-grid-search-label {
        text-align: left;
    }
}

.mx-grid-search-inputs {
    display: flex;
}


